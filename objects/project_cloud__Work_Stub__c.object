<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <fields>
        <fullName>project_cloud__Date__c</fullName>
        <deprecated>false</deprecated>
        <description>Date the stub was created. This is used to flush out old stubs</description>
        <externalId>false</externalId>
        <label>Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>project_cloud__Hash__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CASESAFEID(OwnerId) &amp; CASESAFEID(project_cloud__Parent__c)</formula>
        <label>Hash</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>project_cloud__Parent__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <description>The Id of the Parent Object the stub is related to</description>
        <externalId>true</externalId>
        <label>Parent</label>
        <length>18</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>project_cloud__Time__c</fullName>
        <deprecated>false</deprecated>
        <description>The time elapsed as milliseconds. The stopwatch jquery plugin converts this to hh:mm.</description>
        <externalId>false</externalId>
        <label>Time</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <validationRules>
        <fullName>project_cloud__Date_or_Time_Required</fullName>
        <active>true</active>
        <description>Exactly one of the fields Date or Time must be populated on the stub.</description>
        <errorConditionFormula>IF(ISBLANK(project_cloud__Date__c), 0, 1) + IF(ISBLANK(project_cloud__Time__c), 0, 1) != 1</errorConditionFormula>
        <errorMessage>Exactly one of the fields Date or Time must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>project_cloud__Parent_Salesforce_Id</fullName>
        <active>true</active>
        <description>The Parent field must be a valid Salesforce Id (This is not looked up in the System, only that the format matches a Salesforce Id).</description>
        <errorConditionFormula>IF(
	REGEX(project_cloud__Parent__c, &apos;^[a-zA-Z0-9]+$&apos;), /** must be alphanumeric **/
	IF(
		 LEN(project_cloud__Parent__c) == 18,
		 CASESAFEID(LEFT(project_cloud__Parent__c, 15)) !=  project_cloud__Parent__c, /** if its 18 characters, verify that the 3 digit calculated suffix is valid **/
		 IF (LEN(project_cloud__Parent__c) == 15, /** any 15 digit alphanumeric is valid **/
			  false,
			  true
		  )
	)
	, true
)</errorConditionFormula>
        <errorDisplayField>project_cloud__Parent__c</errorDisplayField>
        <errorMessage>The value of the parent field must be an Id</errorMessage>
    </validationRules>
</CustomObject>
