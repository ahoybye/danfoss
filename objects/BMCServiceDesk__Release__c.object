<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <validationRules>
        <fullName>BMCServiceDesk__ActualDeploymentDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__ActualDeploymentEndDate__c),ISBLANK(BMCServiceDesk__ActualDeploymentEndDate__c) ) ), BMCServiceDesk__ActualDeploymentEndDate__c &lt; BMCServiceDesk__ActualDeploymentStartDate__c )</errorConditionFormula>
        <errorMessage>Actual Deployment End Date must be later than the Actual Deployment Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__ActualImplementationDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__ActualImplementationEndDate__c),ISBLANK(BMCServiceDesk__ActualImplementationEndDate__c) ) ), BMCServiceDesk__ActualImplementationEndDate__c &lt; BMCServiceDesk__ActualImplementationStartDate__c )</errorConditionFormula>
        <errorMessage>Actual Implementation End Date must be later than the Actual Implementation Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Cannot_Update_Closed_Release</fullName>
        <active>true</active>
        <description>This Rule does not allow user to modify record, if it is closed.</description>
        <errorConditionFormula>AND( NOT( BMCServiceDesk__State__c ) , NOT(PRIORVALUE( BMCServiceDesk__State__c ) ), NOT( ISNEW() ))</errorConditionFormula>
        <errorMessage>This release is closed. Reopen the release to modify it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__DeploymentDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__EndDeploymentDate__c),ISBLANK(BMCServiceDesk__EndDeploymentDate__c) ) ), BMCServiceDesk__EndDeploymentDate__c &lt; BMCServiceDesk__StartDeploymentDate__c )</errorConditionFormula>
        <errorMessage>End Deployment Date must be later than the Start Deployment Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__ImplementationDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__EndImplementationDate__c),ISBLANK(BMCServiceDesk__EndImplementationDate__c) ) ), BMCServiceDesk__EndImplementationDate__c &lt; BMCServiceDesk__StartImplementationDate__c )</errorConditionFormula>
        <errorMessage>End Implementation Date must be later than the Start Implementation Date.</errorMessage>
    </validationRules>
</CustomObject>
