<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Defective_Bulletin_Delivery_Job_Required</fullName>
        <active>true</active>
        <description>Defective Bulletin Delivery Job is required if Defect Correction&quot; is set to &quot;true&quot;.</description>
        <errorConditionFormula>AND(ISBLANK(Defective_Bulletin_Delivery_Job__c), Defect_Correction__c )</errorConditionFormula>
        <errorDisplayField>Defective_Bulletin_Delivery_Job__c</errorDisplayField>
        <errorMessage>Please select Defective Bulletin Delivery Job.</errorMessage>
    </validationRules>
</CustomObject>
