    <validationRules>
        <fullName>Defective_Bulletin_Delivery_Job_Required</fullName>
        <active>true</active>
        <description>Defective Bulletin Delivery Job is required if Defect Correction&quot; is set to &quot;true&quot;.</description>
        <errorConditionFormula>AND(ISBLANK(Defective_Bulletin_Delivery_Job__c), Defect_Correction__c )</errorConditionFormula>
        <errorDisplayField>Defective_Bulletin_Delivery_Job__c</errorDisplayField>
        <errorMessage>Please select Defective Bulletin Delivery Job.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Execute</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Execute</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/apex.js&quot;)} 

try { 
if(&apos;{!Bulletin_Delivery_Job__c.Status__c}&apos; == &apos;Completed&apos;){
alert(&apos;This job is already processed&apos;); 
}else{
sforce.apex.execute(&quot;PibUtility&quot;,&quot;executeBatchJobForBDJ&quot;, {bdjId:&apos;{!Bulletin_Delivery_Job__c.Id}&apos;}); 
alert(&apos;{!$Label.Bulletin_Delivery_Job_Processed}&apos;); 
location.reload(); 
}
} catch(e) { 
alert(&apos;Unable to process request. Please retry.\n&apos; + e); 
}</url>
    </webLinks>
