{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/apex.js&quot;)}

try {
sforce.apex.execute(&quot;BulletinDeliveryJobUtility&quot;,&quot;RemoveInCompleteTasks&quot;, {bdjId:&apos;{!Bulletin_Delivery_Job__c.Id}&apos;});
