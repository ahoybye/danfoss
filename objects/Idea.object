    <fields>
        <fullName>Closed__c</fullName>
        <description>Based on the current status, is this idea open or closed?</description>
        <externalId>false</externalId>
        <formula>CASE( Status , &quot;Now Available&quot;, &quot;Yes&quot;,
 &quot;Rejected&quot;, &quot;Yes&quot;,
 &quot;Already Implemented&quot;, &quot;Yes&quot;,
 &quot;Maybe Later&quot;, &quot;Yes&quot;,
 &quot;Under Consideration&quot;, &quot;No&quot;,
 &quot;Coming Soon&quot;, &quot;No&quot;,
 &quot;No&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Closed?</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Closure_Reason__c</fullName>
        <description>Explanation about how the Idea was closed (how it was implemented, why it was rejected, etc).</description>
        <externalId>false</externalId>
        <label>Closure Reason</label>
        <length>32768</length>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Implementation_Date__c</fullName>
