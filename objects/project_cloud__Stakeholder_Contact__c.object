    <fields>
        <fullName>project_cloud__Engagement__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
  ISPICKVAL(project_cloud__Power__c, &apos;High&apos;),
  IF(
    ISPICKVAL(project_cloud__Interest__c, &apos;High&apos;),
    &apos;Manage Closely&apos;,
    IF(
      ISPICKVAL(project_cloud__Interest__c, &apos;Low&apos;),
      &apos;Keep Satisfied&apos;,
      &apos;&apos;
    )
  ),
  IF(
    ISPICKVAL(project_cloud__Power__c, &apos;Low&apos;),
    IF(ISPICKVAL(project_cloud__Interest__c, &apos;High&apos;),
      &apos;Keep Informed&apos;,
      IF(
        ISPICKVAL(project_cloud__Interest__c, &apos;Low&apos;),
        &apos;Monitor&apos;,
        &apos;&apos;
      )
    ), &apos;&apos;
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
