    <fields>
        <fullName>ccpe_r__End__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF($RecordType.DeveloperName == &apos;Weekly&apos;,
	ccpe_r__Start__c + 6,
/*ELSE*/IF($RecordType.DeveloperName == &apos;Monthly&apos;,
	DATE(
		YEAR(ccpe_r__Start__c),
		MONTH(ccpe_r__Start__c),
		CASE(MONTH(ccpe_r__Start__c),
			1, 31,
			2, IF(MOD(YEAR(ccpe_r__Start__c), 4) = 0, 29, 28),
			3, 31,
			4, 30,
			5, 31,
			6, 30,
			7, 31,
			8, 31,
			9, 30,
			10, 31,
			11, 30,
			12, 31,
			0
		)
	),
	NULL
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>End</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
