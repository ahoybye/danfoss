        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Edit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/23.0/connection.js&quot;)}
 
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;
var user = sforce.connection.getUserInfo();
var rec=  new sforce.SObject(&quot;Contract__c&quot;);
rec.Creator = &apos;{!Contract__c.CreatedById}&apos;;
rec.Id = &apos;{!Contract__c.Id}&apos;;
if (&apos;{!Contract__c.CreatedById}&apos;!=user.userId.substring(0, 15) &amp;&amp; &apos;{!Contract__c.Account_Manager__c}&apos;!=user.userFullName &amp;&amp; &apos;{!Contract__c.Sales_Manager_of_Account__c}&apos;!=user.userFullName &amp;&amp;  &apos;{!Contract__c.Contract_Specialist__c}&apos; != user.userFullName &amp;&amp;
user.profileId != &apos;00eC0000001f4zQIAQ&apos; &amp;&amp; user.profileId != &apos;00eC000000192RWIAY&apos;)
{
	alert(&apos;Insufficient privileges to edit.&apos;);
}
else
{
	try
	{
		
		navigateToUrl(&apos;/{!Contract__c.Id}/e?retURL=%2F{!Contract__c.Id}&apos;);		
	}
	catch (err)
		{
			alert(&apos;Error:&apos; + err);
		}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Submit_for_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/23.0/connection.js&quot;)}
 
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;
var user = sforce.connection.getUserInfo();
var rec = new sforce.SObject(&quot;Contract__c&quot;);
rec.Id = &apos;{!Contract__c.Id}&apos;;
if (&apos;{!Contract__c.Contract_Specialist__c}&apos; != user.userFullName &amp;&amp; user.profileId != &apos;00eC0000001f4zQIAQ&apos; &amp;&amp; user.profileId != &apos;00eC000000192RWIAY&apos;)
{
    alert(&apos;Contract must go through stakeholder review before submitting for approval. Only Contract Specialists can submit for approval. Please contact your regional Contract Specialist to submit for approval.&apos;);
}
else if (&apos;{!Contract__c.SubmittedForStakeholderReview__c}&apos; == false)
{
    alert(&apos;Contract must be submitted for stakeholder review before submitting for approval.&apos;);
}
else if ((&apos;{!Contract__c.Reviewed_With_Legal__c}&apos; == false &amp;&amp; &apos;{!Contract__c.Legal_Review_Required__c}&apos; == true) || &apos;{!Contract__c.Business_Impact_Reviewed__c}&apos; == false || &apos;{!Contract__c.Accurate_Contract_Type__c}&apos; == false)
{
    alert(&apos;The Risk Assessment section must be completed per the requirements shown in the yellow box before submitting.&apos;);
}
else if (&apos;{!Contract__c.Submit_for_Approval__c}&apos; == &apos;1&apos;)
{
    alert(&apos;Approval in progress.&apos;);
}
else
{
    try
    {
        rec.Submit_For_Approval__c = true;

        var saveResult = sforce.connection.update([rec]);

        if (saveResult[0].success != &apos;true&apos;)
        {
            alert(&quot;Unable to update : &quot; + saveResult[0].errors.message + &quot;, &quot; + saveResult[0].errors.statusCode);
        }
        else
        {
            navigateToUrl(&apos;/{!Contract__c.Id}&apos;);
        }

    }
    catch (err)
    {
        alert(&apos;Error:&apos; + err);
    }
}</url>
    </webLinks>
    <webLinks>
        <fullName>Submit_for_Stakeholder_Review</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Stakeholder Review</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/23.0/connection.js&quot;)}
 
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;

var rec = new sforce.SObject(&quot;Contract__c&quot;);
rec.Id = &apos;{!Contract__c.Id}&apos;;
if (&apos;{!Contract__c.SubmittedForStakeholderReview__c}&apos; == &apos;1&apos;)
{
    alert(&apos;Contract has already been submitted to the Conract Specialist for audit.&apos;);
}
else if (&apos;{!Contract__c.File_Attached__c}&apos; == &apos;0&apos;)
{
    alert(&apos;There must be a least one document attached before submitting.&apos;);
}
else
{
    try
    {
        rec.SubmittedForStakeholderReview__c= true;

        var saveResult = sforce.connection.update([rec]);

        if (saveResult[0].success != &apos;true&apos;)
        {
            alert(&quot;Unable to update : &quot; + saveResult[0].errors.message + &quot;, &quot; + saveResult[0].errors.statusCode);
        }
        else
        {
            alert(&apos;Contract has been submitted for Stakeholder Review.&apos;);
            navigateToUrl(&apos;/{!Contract__c.Id}&apos;);
        }

    }
    catch (err)
    {
        alert(&apos;Error:&apos; + err);
    }
}</url>
    </webLinks>
