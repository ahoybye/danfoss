<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>AssignToFieldRule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL( Assigned_To__c,&quot;Distributor&quot;), ISBLANK(Account_Manager__c))</errorConditionFormula>
        <errorDisplayField>Assigned_To__c</errorDisplayField>
        <errorMessage>Assign To cannot be distributor unless Parnter Manager is populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_State_Province_if_USA_or_Canada</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(Country__r.Country_Code__c = &quot;CA&quot;, Country__r.Country_Code__c =&quot;US&quot;) ,ISBLANK( State_Province__c ))</errorConditionFormula>
        <errorDisplayField>State_Province__c</errorDisplayField>
        <errorMessage>You must enter a state/province</errorMessage>
    </validationRules>
</CustomObject>
