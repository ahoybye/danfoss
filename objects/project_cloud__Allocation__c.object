    <fields>
        <fullName>project_cloud__Hours__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>/*(End__c - Start__c) * Hours_Day__c*/
project_cloud__Allocation_Group__r.project_cloud__Allocation_Summary__r.project_cloud__Project_Task__r.project_cloud__Estimated_Hours__c * ( project_cloud__Load__c * (IF( ISBLANK(project_cloud__Allocation_Group__r.project_cloud__Load_Override__c), (project_cloud__Allocation_Group__r.project_cloud__Load__c *  project_cloud__Allocation_Group__r.project_cloud__Allocation_Summary__r.project_cloud__Available_Load__c), project_cloud__Allocation_Group__r.project_cloud__Load__c) ) )</formula>
        <label>Hours (Calculated)</label>
        <precision>18</precision>
