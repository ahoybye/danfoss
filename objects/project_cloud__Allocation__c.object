    <fields>
        <fullName>project_cloud__Hours_Day__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>BLANKVALUE(project_cloud__Load_Override__c * project_cloud__Allocation_Group__r.project_cloud__Hours_Day__c,IF(AND(project_cloud__Allocation_Group__r.project_cloud__Balanced_Hours_Day__c &gt; 0.0, project_cloud__Allocation_Group__r.project_cloud__Total_Subscriptions__c &gt; 0),project_cloud__Allocation_Group__r.project_cloud__Balanced_Hours_Day__c / project_cloud__Allocation_Group__r.project_cloud__Total_Subscriptions__c, 0))</formula>
        <label>Hours Per Day (Calculated)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
