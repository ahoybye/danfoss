        <externalId>false</externalId>
        <formula>IF( 
AND(PricebookEntry.Product2.CanUseQuantitySchedule, TEXT(Opportunity.Production_Start_Date__c)!=&quot;&quot;) 
, 
HYPERLINK(
(&quot;/oppitm/establishschedule.jsp?id=&quot; +   Id + &quot;&amp;retURL=&quot; + Id +&quot;&amp;p8=&quot; +  TEXT(DAY(Opportunity.Production_Start_Date__c)) + &quot;-&quot; +  
TEXT(MONTH(Opportunity.Production_Start_Date__c))+ &quot;-&quot; +
TEXT(YEAR(Opportunity.Production_Start_Date__c))
)
, &quot;Establish Schedule with Date of first delivery&quot;)  , 
&quot;&quot;)</formula>
