    <fields>
        <fullName>ccpe_ppm__Cost_Overrun_Score__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Risk__c, &apos;None&apos;),0, 
IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Risk__c, &apos;Low&apos;),1, 
IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Risk__c, &apos;Medium&apos;),2, 
IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Risk__c, &apos;High&apos;),3,0) 
) 
) 
) + 
IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Likelihood__c, &apos;Low&apos;), 0, 
IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Likelihood__c, &apos;Medium&apos;), 1, 
IF(ISPICKVAL(ccpe_ppm__Cost_Overrun_Likelihood__c, &apos;High&apos;), 2,0) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The calculated score with relation to cost overruns determined from risk and likelihood</inlineHelpText>
        <label>Cost Overrun - Score</label>
        <precision>18</precision>
