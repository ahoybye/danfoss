    <validationRules>
        <fullName>Prototype_requires_Opportunity</fullName>
        <active>false</active>
        <description>If reason = prototype, then opportunity field must be filled in.</description>
        <errorConditionFormula>IF( AND( ISPICKVAL( Reason__c , &apos;Prototype&apos;) , Opportunity__c =null), true, false)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>An opportunity must be selected if reason is Prototype.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonOtherRequired</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISPICKVAL(Reason__c, &apos;Other&apos;) &amp;&amp; Reason_Other__c == &apos;&apos;, true, false)</errorConditionFormula>
        <errorDisplayField>Reason_Other__c</errorDisplayField>
