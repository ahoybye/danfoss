    <fields>
        <fullName>Date_Before_Today_in_Month__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 1+(ActivityDate-(DATEVALUE(TEXT(YEAR(ActivityDate)) + &quot;-&quot; + TEXT(MONTH(ActivityDate)) + &quot;-&quot; + &quot;01&quot;))) 
&lt;= 
(1+TODAY()-(DATEVALUE(TEXT(YEAR(TODAY())) + &quot;-&quot; + TEXT(MONTH(TODAY())) + &quot;-&quot; + &quot;01&quot;))) 
, TRUE 
, FALSE 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Date Before Today in Month</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Date_before_Today_in_Quarter__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
CASE( 
IF( MONTH(ActivityDate) &lt;=3, &quot;Q1&quot;, 
IF( MONTH(ActivityDate) &lt;=6, &quot;Q2&quot;, 
IF( MONTH(ActivityDate) &lt;=9, &quot;Q3&quot;, 
&quot;Q4&quot; 
))) , 
&quot;Q1&quot;, ActivityDate-DATE(YEAR(ActivityDate), 1, 1) + 1, 
&quot;Q2&quot;, ActivityDate-DATE(YEAR(ActivityDate), 4, 1) + 1, 
&quot;Q3&quot;, ActivityDate-DATE(YEAR(ActivityDate), 7, 1) + 1, 
ActivityDate-DATE(YEAR(ActivityDate), 10, 1) + 1 
) 
&lt;= 
CASE( IF( MONTH(TODAY()) &lt;=3, &quot;Q1&quot;, 
IF( MONTH(TODAY()) &lt;=6, &quot;Q2&quot;, 
IF( MONTH(TODAY()) &lt;=9, &quot;Q3&quot;, 
&quot;Q4&quot; 
))) , 
&quot;Q1&quot;, TODAY()-DATE(YEAR(TODAY() ), 1, 1) + 1, 
&quot;Q2&quot;, TODAY()-DATE(YEAR(TODAY() ), 4, 1) + 1, 
&quot;Q3&quot;, TODAY()-DATE(YEAR(TODAY() ), 7, 1) + 1, 
TODAY()-DATE(YEAR(TODAY() ), 10, 1) + 1 
) 
, TRUE 
, FALSE 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Date before Today in Quarter</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Date_before_Today_in_Year__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
(1+(ActivityDate- (DATEVALUE( TEXT(YEAR(ActivityDate)) + &quot;-01-01&quot; )))) 
&lt;= 
(1+(TODAY()- (DATEVALUE(TEXT(YEAR(TODAY()))+ &quot;-01-01&quot; )))) 
, TRUE 
, FALSE 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Date before Today in Year</label>
        <type>Checkbox</type>
    </fields>
