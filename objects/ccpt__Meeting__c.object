        <fullName>ccpt__Duration_Actual_Minutes__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ccpt__Start_Actual__c == null,
  0,
  IF(ccpt__isRunning__c,
    MIN(FLOOR(1440 * (NOW() - ccpt__Start_Actual__c)), 1440),
    ccpt__Duration_Actual__c
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Actual Duration (Minutes)</label>
