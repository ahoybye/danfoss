    <fields>
        <fullName>project_cloud__End__c</fullName>
        <deprecated>false</deprecated>
        <description>The end of the Timesheet is 7 days (including the start date) after the start of the Timesheet</description>
        <externalId>false</externalId>
        <formula>project_cloud__Start__c + 6</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The end of the Timesheet is 7 days (including the start date) after the start of the Timesheet</inlineHelpText>
        <label>End</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>project_cloud__Hash__c</fullName>
        <deprecated>false</deprecated>
        <description>This is a hash of the OwnerId of the Timesheet and the start date, it is used by the validation rule to enforce unique timesheets</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(OwnerId) + TEXT(YEAR(project_cloud__Start__c)) + &apos;-&apos; + TEXT(MONTH(project_cloud__Start__c)) + &apos;-&apos; + TEXT(DAY(project_cloud__Start__c))</formula>
        <inlineHelpText>This is a system field and should never be visible.</inlineHelpText>
        <label>Hash</label>
        <required>false</required>
