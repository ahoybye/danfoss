<apex:page Controller="BMCServiceDesk.APMPageController" showHeader="false" >

<script>

var userLanguage = '{!userLanguage}';
var showJapaneseHelp = '{!showJapaneseHelp}';
var param = "{!JSENCODE($CurrentPage.parameters.Id)}";
var wikiUrl = '{!wikiUrl}';
if(param == 'bmcremedyforcehelp'){
    if(typeof(wikiUrl)!= 'undefined' && wikiUrl != null && wikiUrl != '' )
        window.location = wikiUrl;
    else if(userLanguage == 'ja' && showJapaneseHelp.toLowerCase() == 'true')
        window.location="{!$Resource.StdFormHelp_JA}/helpfile.htm";
    else
        window.location="{!$Resource.StdFormHelp}/helpfile.htm";
}
else if(param == 'ReleaseSchedules'){
    window.location = 'https://docs.bmc.com/docs/display/public/REMF/Release+schedules';
}
else if(param == 'upgradeguide')
{
if(userLanguage == 'ja' && showJapaneseHelp.toLowerCase() == 'true')
window.location="{!$Resource.Docs_JA}/UpgradeGuide.pdf";
else
window.location="{!$Resource.UpgradeGuide}/UpgradeGuide.pdf";
}else if(param == 'installguide')
{
if(userLanguage == 'ja' && showJapaneseHelp.toLowerCase() == 'true')
window.location="{!$Resource.Docs_JA}/InstallationandConfigurationGuide.pdf";
else
window.location="{!$Resource.InstallConfigGuide}/InstallationandConfigurationGuide.pdf";
}else if(param == 'releasenotes')
{
if(userLanguage == 'ja' && showJapaneseHelp.toLowerCase() == 'true')
window.location="{!$Resource.Docs_JA}/ReleaseNotes.pdf";
else
window.location="https://communities.bmc.com/docs/DOC-31190";
}   
   
</script>
</apex:page>